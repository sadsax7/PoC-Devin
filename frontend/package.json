{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "type-check": "tsc --noEmit",
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "coverage": "npm run test:coverage"
  },
  "dependencies": {
    "next": "16.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@types/chai": "^4.3.20",
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/sinon": "^17.0.4",
    "autoprefixer": "^10.4.20",
    "chai": "^4.5.0",
    "eslint": "^9.18.0",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-sonarjs": "^3.0.6",
    "eslint-plugin-tsdoc": "^0.5.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdom": "^27.4.0",
    "mocha": "^11.3.0",
    "nyc": "^17.1.0",
    "postcss": "^8.5.1",
    "prettier": "^3.8.1",
    "sinon": "^21.0.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx",
      ".js"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "check-coverage": true,
    "lines": 85,
    "functions": 85,
    "branches": 85,
    "statements": 85
  }
}
